<h1>Timeoff requests</h1>

<table class="table">
  <tr>
    <th>Employee</th>
    <th>Stard date</th>
    <th>End date</th>
    <th>Status</th>
    <th>Action</th>
  </tr>

  <%= for request <- @timeoff_requests do %>
    <tr>
      <td><%= request.employee_uuid %></td>
      <td><%= Date.to_string(request.start_date) %></td>
      <td><%= Date.to_string(request.end_date) %></td>
      <td><%= request.status %></td>
      <td>..</td>
    </tr>
  <% end %>
</table>
